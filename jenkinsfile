pipeline {
    agent any
    stages {
        stage('Delete Branches') {
            steps {
                script {
                    // List of branches to delete
                    def branchesToDelete = ['test-branch-1', 'test-branch-2', 'test-branch-3', 'test-branch-4', 'test-branch-5', 'test-branch-6', 'test-branch-7', 'test-branch-8', 'test-branch-9', 'test-branch-10']

                    // Delete branches remotely and locally
                    branchesToDelete.each { branch ->
                        // Delete branch remotely
                        sh "git push origin --delete ${branch}"

                        // Delete branch locally
                        sh "git branch -D ${branch}"
                    }
                }
            }
        }
    }
}
